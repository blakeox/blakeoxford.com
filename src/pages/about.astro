---
import Layout from '../layouts/BaseLayout.astro';
import PhotoCarousel from '../components/PhotoCarousel.astro';

interface SocialLink {
  name: string;
  url: string;
  icon: 'linkedin' | 'github' | 'microsoft-learn';
}

interface Testimonial {
  name: string;
  title: string;
  image: string;
  quote: string;
}

interface TimelineItem {
  year: string;
  icon: string;
  title: string;
  achievements: string[];
  color: string;
}

const title = "About Me";
const description = "Learn about Blake Oxford, Systems Architect, Workflow Strategist, and Action-Oriented Operator. Explore key skills, career highlights, and collaboration opportunities.";
const canonicalUrl = "https://blakeoxford.com/about/";

// Social Media Links
const socialLinks: SocialLink[] = [
  { name: "LinkedIn", url: "https://www.linkedin.com/in/blakeoxford/", icon: "linkedin" },
  { name: "GitHub", url: "https://github.com/blakeoxford", icon: "github" },
  { name: "Microsoft Learn", url: "https://learn.microsoft.com/users/BlakeOxford-1002", icon: "microsoft-learn" }
];

// Structured data for Person
const personSchema = {
  "@context": "https://schema.org",
  "@type": "Person",
  "name": "Blake Oxford",
  "url": "https://blakeoxford.com",
  "jobTitle": "Systems Architect",
  "sameAs": socialLinks.map(link => link.url),
  "description": description,
  "image": "/assets/images/Blake-O-scaled.jpg"
};

// Testimonials
const testimonials: Testimonial[] = [
  {
    name: "Michael Johnson",
    title: "CEO, InnovaTech",
    image: "/assets/images/placeholder-avatar-1.jpg",
    quote: "Blake's strategic vision transformed our business operations. His implementation of AI-driven workflows resulted in 30% cost reduction while increasing productivity."
  },
  {
    name: "Sarah Williams",
    title: "CTO, HealthTech Solutions",
    image: "/assets/images/placeholder-avatar-2.jpg",
    quote: "Working with Blake on our cloud migration was seamless. His technical knowledge combined with business acumen is rare and incredibly valuable."
  },
  {
    name: "David Chen",
    title: "Director of Finance, Global Systems",
    image: "/assets/images/placeholder-avatar-3.jpg",
    quote: "Blake's financial modeling expertise helped us make critical decisions with confidence. His Power BI dashboards are now essential tools for our executive team."
  }
];

// Timeline Data
const timelineItems: TimelineItem[] = [
  {
    year: "2019",
    icon: "üîÑ",
    title: "System Integration & Optimization",
    achievements: [
      "Streamlined cross-departmental workflows through integration of Sage Intacct and ADP",
      "Reduced manual data entry by 80% through custom API integrations",
      "Implemented automated reconciliation processes, reducing month-end close by 3 days"
    ],
    color: "from-[#10b981] to-purple-500"
  },
  {
    year: "2020",
    icon: "‚òÅÔ∏è",
    title: "Cloud Migration & Digital Transformation",
    achievements: [
      "Led Google Workspace to Microsoft 365 migration for 180 staff with zero downtime",
      "Implemented Sage Intacct and ADP Workforce Now, consolidating 5 legacy systems",
      "Designed disaster recovery protocols reducing potential data loss window by 90%"
    ],
    color: "from-[#10b981] to-purple-500"
  },
  {
    year: "2023",
    icon: "üí°",
    title: "Analytics & Process Innovation",
    achievements: [
      "Developed Power BI dashboards for real-time financial forecasting",
      "Authored SOPs and trained team on AdvancedMD RCM",
      "$1.2M in unbilled services surfaced via SQL-based reporting suite"
    ],
    color: "from-[#10b981] to-purple-500"
  },
  {
    year: "2024",
    icon: "ü§ñ",
    title: "AI-Powered Compliance & Workflow Enhancement",
    achievements: [
      "Developed and launched a real-time Azure OpenAI auditing system for clinical notes, ensuring insurance compliance before submission",
      "Enabled automated coaching for providers and technicians, improving documentation quality and reducing claim denials",
      "Integrated AI into daily workflows for tasks like insurance appeals and internal process optimization, saving hours across departments"
    ],
    color: "from-[#10b981] to-purple-500"
  }
];
---

<Layout 
  title={title} 
  description={description} 
  image="/assets/images/Blake-O-scaled.jpg" 
  url={canonicalUrl} 
  wide={true}
>
  <script type="application/ld+json" set:html={JSON.stringify(personSchema)} />
  <main id="main-content">
    <!-- Progress Indicator -->
    <div class="fixed left-0 top-0 h-1 z-50 bg-gradient-to-r from-accent to-accent-dark" id="progress-bar"></div>

    <div>
      <div class="flex-1">
        <!-- Sticky Navigation -->
        <nav id="sticky-nav" class="sticky top-0 z-40 backdrop-blur-md bg-background/80 dark:bg-background-dark/80 shadow-sm py-4 transition-all duration-300">
          <div class="flex justify-center space-x-4 md:space-x-8 text-sm font-medium">
            <a href="#about-me" class="nav-link px-3 py-2 rounded-md hover:text-accent hover:bg-accent/10 transition-all">About Me</a>
            <a href="#achievements" class="nav-link px-3 py-2 rounded-md hover:text-accent hover:bg-accent/10 transition-all">Achievements</a>
            <a href="#education-skills" class="nav-link px-3 py-2 rounded-md hover:text-accent hover:bg-accent/10 transition-all">Education & Skills</a>
            <a href="#about-timeline" class="nav-link px-3 py-2 rounded-md hover:text-accent hover:bg-accent/10 transition-all">Career Highlights</a>
            <a href="#testimonials" class="nav-link px-3 py-2 rounded-md hover:text-accent hover:bg-accent/10 transition-all">Testimonials</a>
            <a href="#about-social" class="nav-link px-3 py-2 rounded-md hover:text-accent hover:bg-accent/10 transition-all">Connect</a>
          </div>
        </nav>
        <div class="space-y-8 md:space-y-16">

          <!-- Hero -->
          <section
            id="about-me"
            class="relative flex items-center h-[60vh] md:h-[calc(100vh-4rem)] overflow-hidden
                   bg-gradient-to-br from-background via-accent/5 to-background
                   dark:from-background-dark dark:via-accent/10 dark:to-background-dark"
            role="region"
            aria-labelledby="about-me-title"
          >
            <!-- Background decorative elements -->
            <div class="absolute inset-0 overflow-hidden pointer-events-none">
              <div class="absolute top-1/4 left-1/4 w-96 h-96 bg-accent/10 rounded-full blur-3xl animate-pulse-glow"></div>
              <div class="absolute bottom-1/4 right-1/4 w-96 h-96 bg-purple-500/10 rounded-full blur-3xl animate-pulse-glow" style="animation-delay: -2s"></div>
              <!-- Subtle grid pattern -->
              <div class="absolute inset-0 opacity-[0.03] dark:opacity-[0.05]">
                <div class="absolute inset-0" style="background-image: linear-gradient(to right, currentColor 1px, transparent 1px), linear-gradient(to bottom, currentColor 1px, transparent 1px); background-size: 24px 24px;"></div>
              </div>
            </div>

            <div class="relative w-full h-full grid grid-cols-1 md:grid-cols-2">
              <!-- left column ‚Äì intro -->
              <div class="order-1 flex flex-col justify-center max-w-3xl mx-auto px-4 md:px-8 relative z-10">
                <div class="prose lg:prose-xl dark:prose-invert">
                  <h1
                    id="about-me-title"
                    class="font-heading font-bold text-h1 md:text-h1 text-gradient 
                          bg-gradient-to-r from-accent via-accent-dark to-accent bg-clip-text text-transparent mb-6 
                          animate-fadeInUp m-0"
                  >
                    If I'm not wondering, I'm building‚Äîand if I'm not building, I'm wondering.
                  </h1>
                </div>
              </div>
              <!-- right column ‚Äì carousel -->
              <div class="order-2 flex flex-1 items-stretch w-full h-full" role="complementary" aria-label="Photo gallery">
                <div class="relative w-full h-full">
                  <PhotoCarousel class="w-full h-full" />
                  <!-- Subtle gradient overlay -->
                  <div class="absolute inset-0 bg-gradient-to-r from-background/20 via-transparent to-transparent dark:from-background-dark/20 pointer-events-none"></div>
                </div>
              </div>
            </div>

            <!-- Scroll indicator -->
            <div class="absolute bottom-8 left-1/2 transform -translate-x-1/2 animate-bounce hidden md:block z-10">
              <div class="flex flex-col items-center gap-2">
                <span class="text-sm font-medium text-accent dark:text-accent-light">Scroll to explore</span>
                <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6 text-accent animate-pulse" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 14l-7 7m0 0l-7-7m7 7V3" />
                </svg>
              </div>
            </div>
          </section>

          <!-- Achievements -->
          <section id="achievements" class="py-16 relative overflow-hidden bg-surface dark:bg-surface-dark">
            <!-- Background pattern -->
            <div class="absolute inset-0 z-0 opacity-5">
              <div class="absolute inset-0" style="background-image: url('data:image/svg+xml,%3Csvg width='60' height='60' viewBox='0 0 60 60' xmlns='http://www.w3.org/2000/svg'%3E%3Cg fill='none' fill-rule='evenodd'%3E%3Cg fill='%239C92AC' fill-opacity='0.4'%3E%3Cpath d='M36 34v-4h-2v4h-4v2h4v4h2v-4h4v-2h-4zm0-30V0h-2v4h-4v2h4v4h2V6h4V4h-4zM6 34v-4H4v4H0v2h4v4h2v-4h4v-2H6zM6 4V0H4v4H0v2h4v4h2V6h4V4H6z'/%3E%3C/g%3E%3C/g%3E%3C/svg%3E');"></div>
            </div>
            <div class="container mx-auto max-w-screen-xl px-4 sm:px-6 lg:px-8 relative z-10">
              <div class="grid grid-cols-1 gap-8 lg:gap-12">
                <div>

                  <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8">
                    <!-- Achievement 1 -->
                    <div class="bg-surface/80 dark:bg-surface-dark/80 p-8 rounded-xl shadow-lg border border-border dark:border-border-dark hover:shadow-2xl hover:-translate-y-1 transition-all duration-300">
                      <div class="flex items-center justify-center w-16 h-16 rounded-lg bg-gradient-to-br from-accent to-tertiary text-foreground-light text-2xl mb-6">
                        <svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6" />
                        </svg>
                      </div>
                      <h3 class="text-xl font-bold mb-3 text-accent-dark dark:text-accent-light">Enterprise Transformation</h3>
                      <p class="mb-4 text-neutral dark:text-neutral-light">Successfully led Google Workspace to Microsoft 365 migration for 180+ staff with zero downtime, integrating 5 legacy systems.</p>
                      <ul class="space-y-2">
                        <li class="flex items-center gap-2">
                          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-green-50 dark:bg-green-900 aspect-square">
                            <svg class="w-4 h-4 text-green-600 dark:text-green-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 3v2m6-2v2M9 19v2m6-2v2M5 9H3m2 6H3m18-6h-2m2 6h-2M7 19h10a2 2 0 002-2V7a2 2 0 00-2-2H7a2 2 0 00-2 2v10a2 2 0 002 2zM9 9h6v6H9V9z" />
                            </svg>
                          </span>
                          <span class="text-sm">Zero downtime during migration</span>
                        </li>
                        <li class="flex items-center gap-2">
                          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-blue-50 dark:bg-blue-900 aspect-square">
                            <svg class="w-4 h-4 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <circle cx="12" cy="12" r="10" stroke-width="2" />
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6v6l4 2" />
                            </svg>
                          </span>
                          <span class="text-sm">5 legacy systems consolidated</span>
                        </li>
                        <li class="flex items-center gap-2">
                          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-purple-50 dark:bg-purple-900 aspect-square">
                            <svg class="w-4 h-4 text-purple-600 dark:text-purple-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a4 4 0 00-3-3.87M9 20H4v-2a4 4 0 013-3.87m9-4a4 4 0 11-8 0 4 4 0 018 0z" />
                            </svg>
                          </span>
                          <span class="text-sm">100% employee adoption</span>
                        </li>
                      </ul>
                    </div>
                    <!-- Achievement 2 -->
                    <div class="bg-surface/80 dark:bg-surface-dark/80 p-8 rounded-xl shadow-lg border border-border dark:border-border-dark hover:shadow-2xl hover:-translate-y-1 transition-all duration-300">
                      <div class="flex items-center justify-center w-16 h-16 rounded-lg bg-gradient-to-br from-accent to-tertiary text-foreground-light text-2xl mb-6">
                        <svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z" />
                        </svg>
                      </div>
                      <h3 class="text-xl font-bold mb-3 text-accent-dark dark:text-accent-light">Financial Analytics</h3>
                      <p class="mb-4 text-neutral dark:text-neutral-light">Built sophisticated Power BI dashboards for real-time financial forecasting that revealed $1.2M in unbilled services.</p>
                      <ul class="space-y-2">
                        <li class="flex items-center gap-2">
                          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-green-50 dark:bg-green-900 aspect-square">
                            <svg class="w-4 h-4 text-green-600 dark:text-green-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8c-1.657 0-3 .895-3 2s1.343 2 3 2 3 .895 3 2-1.343 2-3 2m0-8c1.11 0 2.08.402 2.599 1M12 8V7m0 1v8m0 0v1m0-1c-1.11 0-2.08-.402-2.599-1M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
                            </svg>
                          </span>
                          <span class="text-sm">$1.2M in unbilled services identified</span>
                        </li>
                        <li class="flex items-center gap-2">
                          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-blue-50 dark:bg-blue-900 aspect-square">
                            <svg class="w-4 h-4 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <circle cx="12" cy="12" r="10" stroke-width="2" />
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6v6l4 2" />
                            </svg>
                          </span>
                          <span class="text-sm">Real-time forecast dashboards</span>
                        </li>
                        <li class="flex items-center gap-2">
                          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-purple-50 dark:bg-purple-900 aspect-square">
                            <svg class="w-4 h-4 text-purple-600 dark:text-purple-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7h8m0 0v8m0-8l-8 8-4-4-6 6" />
                            </svg>
                          </span>
                          <span class="text-sm">25% improved financial accuracy</span>
                        </li>
                      </ul>
                    </div>
                    <!-- Achievement 3 -->
                    <div class="bg-surface/80 dark:bg-surface-dark/80 p-8 rounded-xl shadow-lg border border-border dark:border-border-dark hover:shadow-2xl hover:-translate-y-1 transition-all duration-300">
                      <div class="flex items-center justify-center w-16 h-16 rounded-lg bg-gradient-to-br from-accent to-tertiary text-foreground-light text-2xl mb-6">
                        <svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z" />
                        </svg>
                      </div>
                      <h3 class="text-xl font-bold mb-3 text-accent-dark dark:text-accent-light">AI-Powered Analytics Overhaul</h3>
                      <p class="mb-4 text-neutral dark:text-neutral-light">Led the modernization of enterprise analytics by integrating AI-driven insights, centralizing KPIs, and building real-time dashboards that transformed operational decision-making.</p>
                      <ul class="space-y-2">
                        <li class="flex items-center gap-2">
                          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-green-50 dark:bg-green-900 aspect-square">
                            <svg class="w-4 h-4 text-green-600 dark:text-green-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 3v2m6-2v2M9 19v2m6-2v2M5 9H3m2 6H3m18-6h-2m2 6h-2M7 19h10a2 2 0 002-2V7a2 2 0 00-2-2H7a2 2 0 00-2 2v10a2 2 0 002 2zM9 9h6v6H9V9z" />
                            </svg>
                          </span>
                          <span class="text-sm">Deployed 30+ AI-enhanced dashboards across 6 departments</span>
                        </li>
                        <li class="flex items-center gap-2">
                          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-blue-50 dark:bg-blue-900 aspect-square">
                            <svg class="w-4 h-4 text-blue-600 dark:text-blue-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <circle cx="12" cy="12" r="10" stroke-width="2" />
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6v6l4 2" />
                            </svg>
                          </span>
                          <span class="text-sm">Cut reporting delays by 50% with predictive data models</span>
                        </li>
                        <li class="flex items-center gap-2">
                          <span class="inline-flex items-center justify-center w-6 h-6 rounded-full bg-purple-50 dark:bg-purple-900 aspect-square">
                            <svg class="w-4 h-4 text-purple-600 dark:text-purple-400" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7h8m0 0v8m0-8l-8 8-4-4-6 6" />
                            </svg>
                          </span>
                          <span class="text-sm">Unified metrics and insights for real-time decision-making</span>
                        </li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </section>

          <!-- Education & Skills -->
          <section id="education-skills" class="py-16 bg-gradient-to-b from-background to-surface dark:from-background-dark dark:to-surface-dark relative overflow-hidden">
            <!-- Background geometric shapes -->
            <div class="absolute inset-0 z-0 opacity-20 overflow-hidden">
              <div class="absolute top-1/4 left-1/4 w-64 h-64 bg-accent/30 rounded-full blur-3xl"></div>
              <div class="absolute bottom-1/4 right-1/4 w-80 h-80 bg-purple-500/20 rounded-full blur-3xl"></div>
            </div>
            <div class="container mx-auto max-w-screen-xl px-4 sm:px-6 lg:px-8 relative z-10">
              <div class="max-w-3xl mx-auto text-center mb-12">
                <p class="text-lg text-neutral">
                </p>
              </div>
              <!-- Two column layout -->
              <div class="grid grid-cols-1 gap-0 lg:gap-0">
                <!-- Education Column -->
                <div>
                  <h3 class="text-2xl font-bold border-l-4 border-accent pl-4 mb-6">Education</h3>
                  <div class="bg-surface dark:bg-gray-800 p-6 rounded-xl shadow-lg border border-border dark:border-gray-700 hover:shadow-xl transition-all duration-300 group">
                    <div class="flex items-start">
                      <div class="flex-shrink-0 bg-accent/10 p-3 rounded-lg group-hover:scale-110 transition-transform duration-300">
                        <svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8 text-accent" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                          <path d="M12 14l9-5-9-5-9 5 9 5z" />
                          <path d="M12 14l6.16-3.422a12.083 12.083 0 01.665 6.479A11.952 11.952 0 0012 20.055a11.952 11.952 0 00-6.824-2.998 12.078 12.078 0 01.665-6.479L12 14z" />
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 14l9-5-9-5-9 5 9 5zm0 0l6.16-3.422a12.083 12.083 0 01.665 6.479A11.952 11.952 0 0012 20.055a11.952 11.952 0 00-6.824-2.998 12.078 12.078 0 01.665-6.479L12 14zm-4 6v-7.5l4-2.222" />
                        </svg>
                      </div>
                      <div class="ml-4">
                        <h4 class="font-bold text-xl text-accent-dark dark:text-accent-light group-hover:text-accent transition-colors duration-300">Calvin College</h4>
                        <div class="text-sm text-gray-600 dark:text-gray-400 mb-2">Bachelor of Arts in Finance</div>
                        <p class="text-gray-700 dark:text-gray-300">
                          Focused on Finance with complementary studies in Mandarin Chinese and development economics. Gained experience applying financial modeling and international business concepts through coursework and projects.
                        </p>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </section>

          <!-- Resume Highlights -->
          <section id="resume-highlights" class="py-24 relative bg-gradient-to-br from-accent/5 to-background overflow-hidden">
            <!-- Curved divider at top -->
            <div class="absolute top-0 left-0 w-full overflow-hidden">
            </div>
            <div class="container mx-auto max-w-screen-xl px-4 sm:px-6 lg:px-8 relative z-10 pt-16">
              <div class="max-w-2xl mx-auto mb-16">
                <h3 class="text-2xl font-bold border-l-4 border-accent pl-4 mb-6">Core expertise and significant achievements</h3>
              </div>
              <div class="grid grid-cols-1 md:grid-cols-2 gap-8 items-stretch">
                <!-- Practice Management & RCM -->
                <div class="bg-surface dark:bg-gray-800 rounded-xl shadow-xl p-6 flex flex-col h-full transform transition-all duration-300 hover:shadow-2xl hover:-translate-y-1">
